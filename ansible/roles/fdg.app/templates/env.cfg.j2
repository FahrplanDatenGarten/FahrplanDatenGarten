[general]
database_url = postgres://{{ postgresql_user }}:{{ postgresql_password }}@{{ postgresql_host }}:5432/fdg
debug = False
secret_key = {{ fdg_app_secret }}
static_root = {{ static_root }}
allowed_hosts = {{ ansible_fqdn }},fahrplandatengarten.de

[celery]
result_backend = redis://:{{ redis_password }}@{{ redis_host }}:6379/0
broker_url = redis://:{{ redis_password }}@{{ redis_host }}:6379/0
